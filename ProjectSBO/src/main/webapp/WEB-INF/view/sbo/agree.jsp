	// 가입전 안내문구 팝업 닫기
	$('.btn_close').click(function(e) {
		e.preventDefault();
        self.close();
		location.replace('/store2/login');
	});

	$('.chk_all').click(function() {
		$("input[type='checkbox']").prop('checked', this.checked);
	});

	$('.agree span').click(function() {
		$(this).parent().find("input[type='checkbox']").click();
	});

    $("input[type='checkbox']").change(function() {
        if ($("input[type='checkbox']:checked").not('.chk_all').length != 4) {
            $('.chk_all').prop('checked', false);
        }
    });

	// 다음 단계로
	$(".btn_next").click(function(e) {
		e.preventDefault();

		if ( $("input[type='checkbox']:checked").not('.chk_all').length != 4) {
			alert("모든 약관에 동의하세요.");
			return false;
		}
		location.href = '/sbo/join';
	});

	$(".agree span").click(function(e) {
		e.preventDefault();
		$(this).parent().find("input[type='checkbox']").prop('checked', this.checked);
	});
});
</script>
<script src="/sbo/join/alte/plugins/bootstrap.bundle.min.js"></script>
<script src="/sbo/join/alte/js/adminlte.min.js"></script>
