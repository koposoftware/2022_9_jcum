		<a href="#firstPage" class="btnTop txtHide">TOP</a>

		<script type="text/javascript">
			$('.section6 .txtScene').css('padding-top', $('.section6 .bgScene.mo img').height() + 'px');

			$('#contents').fullpage({
				anchors: ['firstPage', 'secondPage', '3rdPage', '4rdPage', '5rdPage', '6rdPage'],
				navigation: false,
				afterLoad: function (anchorLink, index) {
					if (index == 1){
						$('.btnTop').fadeOut();
					}
				},
				onLeave: function(index, nextIndex, direction){
					if (index == 1 && direction == 'down') {
						$('.btnTop').fadeIn();
						$('.section2 .circle').addClass('animate__fadeInUp');
						$('.section2 .circle > div').addClass('animate__fadeInUp animate__delay-half-0s animate__fast');
						$('.section2 .txt1').addClass('animate__zoomIn animate__delay-1s animate__fast');
						$('.section2 dl').addClass('animate__zoomIn animate__delay-half-1s animate__fast');
					} else	if (index == 2 && direction == 'down') {
						$('.section3 h3').addClass('animate__fadeInUpBig');
						$('.section3 ul li:nth-child(1)').addClass('animate__flipInX animate__delay-1s animate__fast');
						$('.section3 ul li:nth-child(2)').addClass('animate__flipInX animate__delay-half-1s animate__fast');
						$('.section3 ul li:nth-child(3)').addClass('animate__flipInX animate__delay-2s animate__fast');
						$('.section3 ul li:nth-child(4)').addClass('animate__flipInX animate__delay-half-2s animate__fast');
					} else	if (index == 3 && direction == 'down') {
						$('.section4 h3').addClass('animate__fadeInUpBig');
						$('.section4 ul li:nth-child(1)').addClass('animate__flipInX animate__delay-1s animate__fast');
						$('.section4 ul li:nth-child(2)').addClass('animate__flipInX animate__delay-half-1s animate__fast');
						$('.section4 ul li:nth-child(3)').addClass('animate__flipInX animate__delay-2s animate__fast');
						$('.section4 ul li:nth-child(4)').addClass('animate__flipInX animate__delay-half-2s animate__fast');
					} else	if (index == 4 && direction == 'down') {
						$('.section5 h3').addClass('animate__fadeInUpBig');
						$('.section5 .feeTable').addClass('animate__zoomIn animate__delay-1s animate__fast');
						$('.section5 .infoTel .hand').addClass('animate__tada animate__delay-half-1s animate__fast');
					} else	if (index == 5 && direction == 'down') {
						$('.section6 .bgScene').addClass('animate__zoomIn');
						$('.section6 .txtScene').addClass('animate__rotateInDownLeft animate__delay-1s animate__fast');
					}
				},
			});

			//앱 다운로드
			$('.btnAppDown').on('click', function(e) {
				e.preventDefault();
				var _this = $(this);
				if (_this.hasClass('on')) {
					_this.removeClass('on');
					$( $(this).attr('href') ).slideUp();
				} else {
					_this.addClass('on');
					$( $(this).attr('href') ).slideDown();
				}
			});

			//자주묻는 질문
			$('.qTxt').on('click', function (e) {
				e.preventDefault();
				var _li = $(this).parent();
				if ( _li.hasClass('on') ) {
					_li.removeClass('on');
					_li.find('p').slideUp();
				} else {
					$('.section4 ul li').removeClass('on');
					$('.section4 ul li > p').slideUp();
					_li.addClass('on');
					_li.find('p').slideDown();
				}
			});

			//창 리사이즈
			$(window).resize(function() {
				resizeWin();
			});	
		
			function resizeWin() {
				if( $(document).width() < 767 ) {
					$('.section6 .bgScene').hide();
					$('.section6 .bgScene.mo').show();
					$('.section6 .txtScene').css('padding-top', $('.section6 .bgScene.mo img').height() + 'px');
				} else {
					$('.section6 .bgScene').show();
					$('.section6 .bgScene.mo').hide();
					$('.section6 .txtScene').css('padding-top', '0');
				}
			}
		</script>
